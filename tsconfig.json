{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2022",
    "moduleResolution": "bundler",
    "noEmit": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}